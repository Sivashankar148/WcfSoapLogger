<?xml version="1.0"?>
<package >
  <metadata>
    <id>WcfSoapLogger</id>
    <version>0.9.3</version>
    <title>Logging SOAP request/response body at binary level</title>
    <authors>Baurzhan Atanov</authors>
    <owners>Baurzhan Atanov</owners>
    <licenseUrl>https://opensource.org/licenses/mit-license.php</licenseUrl>
    <projectUrl>https://github.com/capslocky/WcfSoapLogger</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Features
* Exact byte content of each request and response
* Including any malformed requests and soap faults
* Each request and response is a separate file
* Works for both web-services and clients
* No code modification needed - just put dll and adjust config file
* Custom handling of byte content in your code

https://github.com/capslocky/WcfSoapLogger
	</description>
    <releaseNotes></releaseNotes>
    <tags>wcf soap log</tags>
  </metadata>
  <files>
    <file src="..\bin\$configuration$\WcfSoapLogger.dll" target="lib\net45\" />
    <file src="..\bin\$configuration$\WcfSoapLogger.pdb" target="lib\net45\" />
    <file src="app.config.install.xdt" target="content" />
    <file src="web.config.install.xdt" target="content" />
    <file src="readme.txt" target="" />
  </files>
</package>